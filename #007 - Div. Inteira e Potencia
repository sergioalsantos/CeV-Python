num1 = int(input('Digite um valor..:'))
num2 = int(input('Digite outro valor..:'))

s = num1 + num2
m = num1 * num2
d = num1 / num2
di = num1 // num2
e = num1 ** num2
print('A soma é {}, o produto é {} e a divisão é {:.1f}'.format(s, m, d), end='>>>>>')
print('A Divisão Interira {} e a potência é {:.f}'.format(di, e))
